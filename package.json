{
  "name": "wp-starter-theme",
  "version": "1.0.0",
  "description": "Wordpress Starter Theme, **Project to define hahaha**. Based on TwentyTwenty Theme",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "old-build": "webpack --mode=production --config webpack.config.js --progress",
    "old-dev": "webpack --watch --info-verbosity verbose --mode=development --config webpack.config.js",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=dist,docs,node_modules,src,vendor --textdomain=ecrannoir",
    "i18n:pot": "wpi18n makepot --exclude=dist,docs,node_modules,src,vendor --main-file=style.css --domain-path=languages/ --pot-file=ecrannoir.pot --type=wp-theme --textdomain=ecrannoir",
    "i18n:mo": "msgfmt -o languages/nl_BE.mo languages/nl_BE.po"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noveni/wp-starter-theme.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noveni/wp-starter-theme/issues"
  },
  "homepage": "https://github.com/noveni/wp-starter-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@epegzz/sass-vars-loader": "^6.0.0",
    "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
    "@wordpress/api-fetch": "^3.20.0",
    "@wordpress/babel-preset-default": "^4.19.0",
    "@wordpress/block-editor": "^3.11.0",
    "@wordpress/components": "^9.9.0",
    "@wordpress/data": "^4.25.0",
    "@wordpress/date": "^3.12.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.8.0",
    "@wordpress/element": "^2.18.0",
    "@wordpress/i18n": "^3.16.0",
    "@wordpress/url": "^2.19.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^5.0.2",
    "laravel-mix": "^5.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "node-wp-i18n": "^1.2.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-wordpress": "^15.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "hamburgers": "^1.1.3",
    "lazysizes": "^5.2.2"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ]
}
